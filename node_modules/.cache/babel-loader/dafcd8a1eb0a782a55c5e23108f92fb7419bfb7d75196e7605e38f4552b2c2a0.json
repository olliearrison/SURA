{"ast":null,"code":"import { Scene, PerspectiveCamera, WebGLRenderer, BoxGeometry, MeshBasicMaterial, Mesh } from 'three';\nimport { renderer, camera, scene, plane, drawSceneList, drawRenderer } from '../App.vue';\nexport default {\n  data() {\n    return {\n      model: null\n    };\n  },\n  mounted() {\n    this.initializeScene();\n  },\n  methods: {\n    initializeScene() {\n      // Create a scene\n      const scene = new Scene();\n\n      // Create a camera\n      const camera = new PerspectiveCamera(75, 1, 0.1, 1000);\n      camera.position.z = 5;\n\n      // Create a renderer\n      const renderer = new WebGLRenderer({\n        antialias: true\n      });\n      const cardElements = this.$refs.cardElements;\n      const width = cardElements[0].offsetWidth;\n      const height = cardElements[0].offsetHeight;\n      renderer.setSize(width, height);\n      cardElements.forEach(cardElement => {\n        const cardContentElement = cardElement.querySelector('.card-content');\n        cardContentElement.appendChild(renderer.domElement);\n      });\n\n      // Create a cube\n      const geometry = new BoxGeometry();\n      const material = new MeshBasicMaterial({\n        color: 0x00ff00\n      });\n      const cube = new Mesh(geometry, material);\n      scene.add(cube);\n\n      // Render the scene\n      function animate() {\n        requestAnimationFrame(animate);\n        cube.rotation.x += 0.01;\n        cube.rotation.y += 0.01;\n        renderer.render(scene, camera);\n      }\n      animate();\n    }\n  }\n};","map":{"version":3,"names":["Scene","PerspectiveCamera","WebGLRenderer","BoxGeometry","MeshBasicMaterial","Mesh","renderer","camera","scene","plane","drawSceneList","drawRenderer","data","model","mounted","initializeScene","methods","position","z","antialias","cardElements","$refs","width","offsetWidth","height","offsetHeight","setSize","forEach","cardElement","cardContentElement","querySelector","appendChild","domElement","geometry","material","color","cube","add","animate","requestAnimationFrame","rotation","x","y","render"],"sources":["/Users/olliearrison/Desktop/SURA/src/components/LayerViewer.vue"],"sourcesContent":["<template>\n    <div class=\"layer-viewer\">\n      <v-sheet class=\"mx-auto\" elevation=\"0\" max-width=\"800\">\n        <v-slide-group\n          v-model=\"model\"\n          class=\"pa-4\"\n          show-arrows\n        >\n          <v-slide-group-item\n            v-for=\"n in 3\"\n            :key=\"n\"\n          >\n            <div class=\"card-wrapper\" ref=\"cardElements\">\n              <v-card\n                width=\"100\"\n                height=\"100\"\n                class=\"transparent-card ma-4\"\n              >\n                <div class=\"card-content\">\n                  <div class=\"d-flex fill-height align-center justify-center\">\n                  </div>\n                </div>\n              </v-card>\n            </div>\n          </v-slide-group-item>\n        </v-slide-group>\n      </v-sheet>\n    </div>\n  </template>\n  \n  <script>\n  import { Scene, PerspectiveCamera, WebGLRenderer, BoxGeometry, MeshBasicMaterial, Mesh } from 'three';\n  import { renderer, camera, scene, plane, drawSceneList, drawRenderer } from '../App.vue';\n  \n  export default {\n    data() {\n      return {\n        model: null,\n      };\n    },\n    mounted() {\n      this.initializeScene();\n    },\n    methods: {\n      initializeScene() {\n        // Create a scene\n        const scene = new Scene();\n  \n        // Create a camera\n        const camera = new PerspectiveCamera(75, 1, 0.1, 1000);\n        camera.position.z = 5;\n  \n        // Create a renderer\n        const renderer = new WebGLRenderer({ antialias: true });\n        const cardElements = this.$refs.cardElements;\n        const width = cardElements[0].offsetWidth;\n        const height = cardElements[0].offsetHeight;\n        renderer.setSize(width, height);\n        cardElements.forEach((cardElement) => {\n          const cardContentElement = cardElement.querySelector('.card-content');\n          cardContentElement.appendChild(renderer.domElement);\n        });\n  \n        // Create a cube\n        const geometry = new BoxGeometry();\n        const material = new MeshBasicMaterial({ color: 0x00ff00 });\n        const cube = new Mesh(geometry, material);\n        scene.add(cube);\n  \n        // Render the scene\n        function animate() {\n          requestAnimationFrame(animate);\n          cube.rotation.x += 0.01;\n          cube.rotation.y += 0.01;\n          renderer.render(scene, camera);\n        }\n        animate();\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .layer-viewer {\n    position: fixed;\n    bottom: 0;\n    width: 100%;\n    height: 8%;\n    z-index: 1100;\n    align-items: center;\n    justify-content: center;\n    display: flex;\n  }\n  \n  .v-card {\n    background-color: transparent;\n    box-shadow: none;\n  }\n  \n  .transparent-card {\n    background-color: transparent;\n    border: none;\n  }\n\n  .v-sheet {\n    position: fixed;\n    bottom: 0;\n    background-color: rgba(1, 1, 1, 0.7);\n    color: rgb(176, 190, 197);\n    border-radius: 5px;\n  }\n  \n  .card-wrapper {\n    display: inline-block;\n  }\n  \n  .card-content {\n    width: 100%;\n    height: 100%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n  }\n  </style>\n  "],"mappings":"AA+BE,SAASA,KAAK,EAAEC,iBAAiB,EAAEC,aAAa,EAAEC,WAAW,EAAEC,iBAAiB,EAAEC,IAAG,QAAS,OAAO;AACrG,SAASC,QAAQ,EAAEC,MAAM,EAAEC,KAAK,EAAEC,KAAK,EAAEC,aAAa,EAAEC,YAAW,QAAS,YAAY;AAExF,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACPD,eAAeA,CAAA,EAAG;MAChB;MACA,MAAMP,KAAI,GAAI,IAAIR,KAAK,CAAC,CAAC;;MAEzB;MACA,MAAMO,MAAK,GAAI,IAAIN,iBAAiB,CAAC,EAAE,EAAE,CAAC,EAAE,GAAG,EAAE,IAAI,CAAC;MACtDM,MAAM,CAACU,QAAQ,CAACC,CAAA,GAAI,CAAC;;MAErB;MACA,MAAMZ,QAAO,GAAI,IAAIJ,aAAa,CAAC